{
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Sharifur/paymentgateway.git"
        }
    ],

    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^2.8",
        "barryvdh/laravel-dompdf": "1.0.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/support": "^9.0",
        "intervention/image": "^2.5",
        "kkomelin/laravel-translatable-string-exporter": "^1.11",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.15",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "mews/purifier": "^3.3",
        "spatie/laravel-feed": "^4.1.0",
        "spatie/laravel-permission": "^5.5.0",
        "spatie/laravel-sitemap": "^6.1.0",
        "symfony/http-client": "^6.3",
        "symfony/mailgun-mailer": "^6.3",
        "symfony/postmark-mailer": "^6.3",
        "xgenious/paymentgateway": "^3.0.17",
        "xgenious/xgapiclient": "*",
        "yajra/laravel-datatables-oracle": "~10.2.3"
    },
    "require-dev": {
        "spatie/laravel-ignition": "1.0",
        "laravel/ui": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
